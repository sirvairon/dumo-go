<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminController">
   <children>
      <VBox>
         <children>
            <Label text="Nom de usuari" />
            <TextField fx:id="textFieldNomUsuari" prefWidth="250.0" />
         </children>
      </VBox>
      <HBox>
         <children>
            <Button fx:id="butoBuscar" mnemonicParsing="false" text="Buscar" />
            <Button fx:id="butoCancelar" mnemonicParsing="false" text="Cancelar" />
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@../styles/general.css" />
      <URL value="@../styles/usuaribuscar.css" />
   </stylesheets>
</VBox>
